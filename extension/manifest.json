{
	"name": "GitHub Notifier",
	"description": "Notifies you about new events on GitHub",
	"version": "1.0.1",
	"background_page": "background.html",
	"permissions": [
		"https://github.com/"
	],
	"browser_action": {
		"default_icon": "icon-19.png"
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"minimum_chrome_version": "16"
}